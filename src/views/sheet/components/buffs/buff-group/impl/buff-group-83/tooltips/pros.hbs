<div class="p-20">
  <div class="fx-layout-row fx-align--center-x fx-align--x-center fx-layout-wrap fx-gap--15px">
    {{#each pros}}
      <div class="item-wrapper fx-layout-column fx-align--start-x fx-align--x-center">
        <img title="{{pro.code}}" draggable="false" src="data:image/png;base64,{{image}}" />
        <div class="fs-3">{{pro.name}}</div>
      </div>
    {{/each}}
  </div>
</div>